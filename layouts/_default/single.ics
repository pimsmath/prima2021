BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//prima-2022//speaker calendar//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
BEGIN:VTIMEZOME
TZID:America/Vancouver
TZURL:http://tzurl.org/zoneinfo-outlook/America/Vancouver
X-LIC-LOCATION:America/Vancouver
BEGIN:DAYLIGHT
TZOFFSETFROM:-0800
TZOFFSETTO:-0700
TZNAME:PDT
DTSTART:19700308T020000
RRULE:FREQ=YEARLY;BYMONTH=3;BYDAY=2SU
END:DAYLIGHT
BEGIN:STANDARD
TZOFFSETFROM:-0700
TZOFFSETTO:-0800
TZNAME:PST
DTSTART:19701101T020000
RRULE:FREQ=YEARLY;BYMONTH=11;BYDAY=1SU
END:STANDARD
END:VTIMEZONE
BEGIN:VEVENT
DTSTAMP;TZID=America/Vancouver:{{dateFormat "20060102T150405" .Date}}
DTSTART;TZID=America/Vancouver:{{dateFormat "20060102T150405" .Date}}
{{ with .Params.end }}DTEND;TZID=America/Vancouver:{{dateFormat "20060102T150405" .}}{{ end }}
{{ with .Params.organizer}}ORGANIZER;CN="{{ .name }}":mailto:{{ .email }}{{ end }}
UID:{{dateFormat "20060102T150405" .Date}}@prima2022.primamath.org
SUMMARY:{{.Title}}
DESCRIPTION:{{.Params.abstract}}
STATUS:CONFIRMED
LOCATION:{{with .Params.locations}}{{ delimit . ";" }}{{else}}TBD{{end}}
END:VEVENT
END:VCALENDAR
